{
  "manifest_version": 1,
  "name": "YouTube Sentiment",
  "version": "1.0",
  
  "description": "Shows YoutTube sentiment based off of the likes to view ratio",
  
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "css": ["styles.css"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
  